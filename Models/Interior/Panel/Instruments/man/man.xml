<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList> 

  <path>man.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/Heston-Type5/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>/engines/engine[0]/mp-inhg</property>
    <interpolation>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind> 15 </ind><dep>  75 </dep></entry>
      <entry><ind> 20 </ind><dep> 110 </dep></entry>
      <entry><ind> 25 </ind><dep> 145 </dep></entry>
      <entry><ind> 30 </ind><dep> 180 </dep></entry>
      <entry><ind> 35 </ind><dep> 215 </dep></entry>
      <entry><ind> 40 </ind><dep> 250 </dep></entry>
      <entry><ind> 45 </ind><dep> 285 </dep></entry>
      <entry><ind> 50 </ind><dep> 320 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
